						<DataTemplate>
							<TextBlock VerticalAlignment="Center"
									   Margin="0"
									   Padding="3"
									   Text="{x:Static resources:Labels.Label_AllocatedToResources}"
									   ToolTip.Tip="{x:Static resources:ToolTips.ToolTip_AllocatedToResources}"/>
						</DataTemplate>
					</DataGridTemplateColumn.HeaderTemplate>
