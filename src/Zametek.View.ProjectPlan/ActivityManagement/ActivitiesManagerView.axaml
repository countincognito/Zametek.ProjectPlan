								  Classes="editable">
								<TextBox IsVisible="{Binding Path=!ShowDates, Mode=OneWay}"
										 VerticalAlignment="Center"
										 TextAlignment="Left"
										 Margin="0"
										 Padding="3"
										 Classes="displayOnly"
										 Text="{Binding Path=MaximumLatestFinishTime, Mode=OneWay}"/>

								<TextBox IsVisible="{Binding Path=ShowDates, Mode=OneWay}"
										 VerticalAlignment="Center"
										 TextAlignment="Center"
										 Margin="0"
										 Padding="3"
										 Classes="displayOnly"
										 Text="{Binding Path=MaximumLatestFinishDateTime, Mode=OneWay, StringFormat={}{0:d}}}"/>
							</Grid>
						</DataTemplate>
					</DataGridTemplateColumn.CellTemplate>
					<DataGridTemplateColumn.CellEditingTemplate>
						<DataTemplate>
							<Grid VerticalAlignment="Stretch">
								<u:NumericIntUpDown IsVisible="{Binding Path=!ShowDates, Mode=OneWay}"
													Value="{Binding Path=MaximumLatestFinishTime, Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
													VerticalAlignment="Center"
													ShowButtonSpinner="False"
													Focusable="True"
													Margin="0"
													Padding="0"
													Minimum="0"/>

								<CalendarDatePicker IsVisible="{Binding Path=ShowDates, Mode=OneWay}"
													SelectedDate="{Binding Path=MaximumLatestFinishDateTime, Mode=TwoWay}"
													Margin="0"
													Padding="3"
													Focusable="True"
													IsTodayHighlighted="True"
													VerticalAlignment="Center"/>
							</Grid>
						</DataTemplate>
					</DataGridTemplateColumn.CellEditingTemplate>
				</DataGridTemplateColumn>

				<DataGridTemplateColumn CanUserResize="True"
										CanUserReorder="True"
										CanUserSort="True"
										CustomSortComparer="{StaticResource workStreamSelectorTargetWorkStreamsStringSortComparer}"
										Width="155">
					<DataGridTemplateColumn.HeaderTemplate>
						<DataTemplate>
							<TextBlock VerticalAlignment="Center"
									   Margin="0"
									   Padding="3"
									   Text="{x:Static resources:Labels.Label_TargetWorkStreams}"
									   ToolTip.Tip="{x:Static resources:ToolTips.ToolTip_TargetWorkStreams}"/>
						</DataTemplate>
					</DataGridTemplateColumn.HeaderTemplate>
					<DataGridTemplateColumn.CellTemplate>
						<DataTemplate>
							<Grid VerticalAlignment="Stretch"
								  Classes="editable">
								<TextBox VerticalAlignment="Center"
										 Margin="0"
										 Padding="3"
										 x:DataType="vm:ManagedActivityViewModel"
										 Classes="displayOnly"
										 Text="{Binding Path=WorkStreamSelector.TargetWorkStreamsString, Mode=OneWay}"/>
							</Grid>
						</DataTemplate>
					</DataGridTemplateColumn.CellTemplate>
					<DataGridTemplateColumn.CellEditingTemplate>
						<DataTemplate>
							<u:MultiComboBox x:DataType="vm:ManagedActivityViewModel"
											 MaxHeight="200"
											 Width="{x:Static sys:Double.NaN}"
											 Focusable="True"
											 HorizontalAlignment="Stretch"
											 VerticalAlignment="Stretch"
											 ItemsSource="{Binding Path=WorkStreamSelector.TargetWorkStreams, Mode=OneWay}"
											 SelectedItems="{Binding Path=WorkStreamSelector.SelectedTargetWorkStreams, Mode=OneWay}"
											 DisplayMemberBinding="{Binding Path=DisplayName, Mode=OneWay}"
											 IsDropDownOpen="True">
								<u:MultiComboBox.SelectedItemTemplate>
									<DataTemplate x:DataType="vm:SelectableWorkStreamViewModel">
										<Grid>
											<TextBlock Text="{Binding Path=DisplayName, Mode=OneWay}"/>
										</Grid>
									</DataTemplate>
								</u:MultiComboBox.SelectedItemTemplate>
							</u:MultiComboBox>
						</DataTemplate>
					</DataGridTemplateColumn.CellEditingTemplate>
				</DataGridTemplateColumn>

				<DataGridTemplateColumn CanUserResize="True"
										CanUserReorder="True"
										CanUserSort="True"
										CustomSortComparer="{StaticResource resourceSelectorTargetResourcesStringSortComparer}"
										Width="130">
					<DataGridTemplateColumn.HeaderTemplate>
						<DataTemplate>
							<TextBlock VerticalAlignment="Center"
									   Margin="0"
									   Padding="3"
									   Text="{x:Static resources:Labels.Label_TargetResources}"
									   ToolTip.Tip="{x:Static resources:ToolTips.ToolTip_TargetResources}"/>
						</DataTemplate>
					</DataGridTemplateColumn.HeaderTemplate>
					<DataGridTemplateColumn.CellTemplate>
						<DataTemplate>
							<Grid VerticalAlignment="Stretch"
								  Classes="editable">
								<TextBox VerticalAlignment="Center"
										 Margin="0"
										 Padding="3"
										 x:DataType="vm:ManagedActivityViewModel"
										 Classes="displayOnly"
										 Text="{Binding Path=ResourceSelector.TargetResourcesString, Mode=OneWay}"/>
							</Grid>
						</DataTemplate>
					</DataGridTemplateColumn.CellTemplate>
